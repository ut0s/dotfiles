{
	"servers": {
		"serena": {
			"command": "uvx",
			"args": [
				"--from",
				"git+https://github.com/oraios/serena",
				"serena",
				"start-mcp-server",
				"--context",
				"ide-assistant",
        "--enable-web-dashboard",
        "False"
			],
			"type": "stdio"
		},
		"github": {
			"url": "https://api.githubcopilot.com/mcp/",
			"type": "http"
		},
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			]
		}
	},
	"inputs": []
}
